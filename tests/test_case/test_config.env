# Test Environment Configuration File
# This is a sample .env file for testing EVM

# Application Settings
APP_NAME="Test Application"
APP_VERSION="1.0.0"
APP_ENV="development"

# Database Configuration
DATABASE_URL="postgresql://localhost:5432/testdb"
DATABASE_USER="testuser"
DATABASE_PASSWORD="testpass"
DATABASE_POOL_SIZE="10"

# Cache Configuration
REDIS_URL="redis://localhost:6379/0"
MEMCACHED_HOST="localhost"
MEMCACHED_PORT="11211"

# API Configuration
API_KEY="test_api_key_67890"
API_URL="https://api.example.com"
API_TIMEOUT="30000"
API_RETRIES="3"

# Feature Flags
DEBUG="true"
VERBOSE="false"
ENABLE_CACHING="true"
ENABLE_LOGGING="true"

# Logging
LOG_LEVEL="debug"
LOG_FILE="/var/log/test-app.log"
LOG_MAX_SIZE="100MB"

# Security
JWT_SECRET="test_jwt_secret_key"
ENCRYPTION_KEY="test_encryption_key"
SESSION_TIMEOUT="3600"

# External Services
EMAIL_SERVICE_URL="smtp://smtp.example.com:587"
SMS_SERVICE_URL="https://sms.example.com/api"
